@model List<Mooshack_2.Models.UserViewModel>
@{
    ViewBag.Title = "showAllUsers";
}

<h2>showAllUsers</h2>

<div class="table-responsive">
    <table class="table">
        <tr>
            <th>
                Username
            </th>
            <th>
                Email
            </th>
        </tr>
        @foreach(var user in Model)
        {
          <tr>
              <td>
                  @Html.DisplayFor(modelItem =>user.UserName)
              </td>
              <td>
                  @Html.DisplayFor(modelItem => user.Email)
              </td>
              
          </tr>
        }
    </table>
</div>
