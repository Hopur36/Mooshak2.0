 @model List<Mooshack_2.Models.ViewModels.CourseViewModel >

@{
    ViewBag.Title = "_listOfCourses";
}

<h2>_listOfCourses</h2>

<!---->
<div class="table-responsive">
    <table class="table">
        <tr>
            <th>
                Title
            </th>
        </tr>

        @foreach (var _course in Model)
        {
            <tr>
                <td>
                   @Html.DisplayFor(modelItem => _course.Name)
                </td>
            </tr>
        }
    </table>
</div>